.overdrive-styling ObjectField VisualElement {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
}

.overdrive-styling ObjectField .unity-object-field__selector {
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: var(--clickable-default);
}

.overdrive-styling ObjectField Label {
    color: var(--clickable-default);
}

.overdrive-styling ObjectField:hover Label {
    color: var(--clickable-hover);
}

.overdrive-styling ObjectField .unity-object-field__selector:hover {
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: var(--clickable-hover);
}

.overdrive-styling ScrollView .unity-repeat-button {
    display: none;
}

.overdrive-styling ScrollView #unity-slider #unity-dragger {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    background-color: var(--clickable-default);
    opacity: 0.13;
    width: 4px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

.overdrive-styling ScrollView #unity-slider #unity-tracker {
    width: 4px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.overdrive-styling ScrollView #unity-slider {
    width: 4px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.overdrive-styling ScrollView #unity-dragger-border {
    display: none;
}

.overdrive-styling ScrollView .spaced-element {
    margin-top: var(--panel-padding);
}

.overdrive-styling ScrollView Scroller {
    display: none;
    width: auto;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
