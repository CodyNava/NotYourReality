.overdrive-styling TextField #unity-text-input {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
}

.overdrive-styling Toggle > Label {
    flex-grow: 1;
    white-space: normal;
    flex-shrink: 1;
    min-width: auto;
}

.overdrive-styling Toggle > .unity-toggle__input {
    flex-grow: 0;
    flex-shrink: 0;
}

.overdrive-styling * {
    -unity-font-definition: var(--font-mono);
}
